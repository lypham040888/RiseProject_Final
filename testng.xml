			<class name="project.AddProjectTestCases" />
		</classes>
	</test>
	<test name="Regression Test All class on edge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="project.AddProjectTestCases" />
		</classes>
	</test>
</suite> <!-- Suite -->
